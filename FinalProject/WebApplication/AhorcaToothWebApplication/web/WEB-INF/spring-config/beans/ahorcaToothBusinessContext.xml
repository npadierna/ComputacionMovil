        http://www.springframework.org/schema/beans/spring-beans-3.0.xsd" >

    <bean
        name="categoryProcess"
        class="co.edu.udea.compumovil.ahorcatooth.process.impl.CategoryProcessImpl"
        scope="session" >
    </bean>

    <bean
        name="hangmanWordProcess"
        class="co.edu.udea.compumovil.ahorcatooth.process.impl.HangmanWordProcessImpl"
        scope="session" >
    </bean>

    <bean
        name="languagesProcess"
        class="co.edu.udea.compumovil.ahorcatooth.process.impl.LanguagesProcessImpl"
        scope="session" >
    </bean>
</beans>