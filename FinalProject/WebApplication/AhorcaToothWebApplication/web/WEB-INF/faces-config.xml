<?xml version='1.0' encoding='UTF-8'?>
<faces-config
    version="2.1" xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
        http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd" >

    <application>
        <el-resolver>
            org.springframework.web.jsf.el.SpringBeanFacesELResolver
        </el-resolver>
    </application>
    <managed-bean>
        <managed-bean-class>
            co.edu.udea.compumovil.ahorcatooth.beans.CategoryBean
        </managed-bean-class>
        <managed-bean-name>categoryBean</managed-bean-name>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-class>
            co.edu.udea.compumovil.ahorcatooth.beans.HangmanWordBean
        </managed-bean-class>
        <managed-bean-name>hangmanWordBean</managed-bean-name>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-class>
            co.edu.udea.compumovil.ahorcatooth.beans.LanguagesBean
        </managed-bean-class>
        <managed-bean-name>languagesBean</managed-bean-name>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>